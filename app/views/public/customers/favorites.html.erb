<!--いいねした投稿の一覧表示-->
<div class="container m-3">
    <div class="row">
        <div class="col">
            <h5><%= @customer.name%>のいいねした投稿一覧</h5>
            <% @favorite_articles.each do |article|%>
                <p><%= link_to article.title, article_path(article) %> </p>
            <% end %>
        </div>
    </div>
</div>